        <uuid>ee56964f-193c-44ae-aa4b-fd5e9d5f7461</uuid>
        <description></description>
        <parentUuid>_a-0000d62a-9e0e-8000-ac7a-680000680000_7543</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;SYSTEM_SYSRULES_textField&quot;(
  label: index(ri!columnAttributeLabels, ri!index, &quot;&quot;),
  value: index(ri!columnAttributeValues, ri!index, &quot;&quot;),
  saveInto: {
    a!save(
      target: ri!columnAttributeValues,
      value: updatearray(
        ri!columnAttributeValues, 
        ri!index,
        save!value
      )
    ),
    /* Make sure that the first column is full before appending values from second column */
    if(
      and(ri!column = &quot;second&quot;, length(local!rightColumnAttributes) &lt; 16),
      a!save(
        target: local!leftColumnAttributes,
        value: updatearray(
          local!leftColumnAttributes, 
          15,
          null
        )
      ),
      {}
    ),
    a!save(
      target: ri!attributeValues,
      value: if(
        ri!column = &quot;first&quot;,
        append(ri!columnAttributeValues, local!rightColumnAttributes),
        append(local!leftColumnAttributes, ri!columnAttributeValues)
      )
    )
  },
  required: contains(#&quot;_a-0000db98-7620-8000-5130-010000010000_2473&quot;, index(ri!columnAttributeLabels, ri!index, &quot;&quot;))
)</definition>
